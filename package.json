{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "gatsby": "^2.8.5",
    "gatsby-image": "^2.1.2",
    "gatsby-plugin-env-variables": "^1.0.1",
    "gatsby-plugin-manifest": "^2.1.1",
    "gatsby-plugin-netlify": "^2.0.17",
    "gatsby-plugin-offline": "^2.1.1",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-robots-txt": "^1.4.0",
    "gatsby-plugin-sharp": "^2.1.3",
    "gatsby-plugin-sitemap": "^2.1.0",
    "gatsby-plugin-styled-components": "^3.0.7",
    "gatsby-plugin-typescript": "^2.0.15",
    "gatsby-plugin-typescript-checker": "^1.0.3",
    "gatsby-source-filesystem": "^2.0.38",
    "gatsby-transformer-sharp": "^2.1.21",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "styled-components": "^4.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@storybook/addon-actions": "^5.1.3",
    "@storybook/addon-knobs": "^5.1.3",
    "@storybook/addon-links": "^5.1.3",
    "@storybook/addons": "^5.1.3",
    "@storybook/react": "^5.1.3",
    "@types/enzyme": "^3.9.3",
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.7",
    "@types/react": "^16.9.36",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^5.0.8",
    "@types/storybook__addon-knobs": "^5.0.1",
    "@types/storybook__react": "^4.0.2",
    "@types/styled-components": "^4.1.16",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.0",
    "babel-preset-gatsby": "^0.1.11",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "gatsby-plugin-tslint": "^0.0.2",
    "husky": "^2.4.1",
    "jest": "^24.8.0",
    "jest-styled-components": "^6.3.1",
    "prettier": "^1.18.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.2",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-loader": "^3.5.4",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.5.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "npm run build:storybook && npm run build:gatsby",
    "build:gatsby": "gatsby build",
    "build:storybook": "NODE_ENV=production build-storybook -s static -o static/storybook",
    "develop": "gatsby develop",
    "develop:storybook": "NODE_ENV=production start-storybook -p 8001 -s static",
    "start": "npm run develop",
    "start:storybook": "npm run develop:storybook",
    "serve": "gatsby serve",
    "format": "prettier --list-different \"src/**/*.{js,jsx,ts,tsx}\"",
    "validate": "tsc --noEmit",
    "lint": "npm run lint:ts && npm run lint:js",
    "lint:ts": "tslint -t stylish \"src/**/*.{ts,tsx}\"",
    "lint:js": "eslint -f stylish \"**/*.{js,jsx}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
